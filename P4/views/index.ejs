<% let title="Template engine" %>
    <%= title%>

        <br>
        <%=caption %>
            <table>
                <tr>
                    <th>Nomor</th>
                    <th>NPM</th>
                </tr>
                <% data.forEach((element, Nomor)=> { %>
                    <tr>
                        <td>
                            <%= Nomor+=1 %>
                        </td>
                        <td>
                            <%= element.npm %>
                        </td>
                    </tr>
                    <% }); %>
            </table>